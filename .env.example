# Database Configuration
DATABASE_URL=jdbc:postgresql://your-db-host:5432/lunch
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your_password_here

# Azure OAuth 2.0
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_ISSUER_URI=https://login.microsoftonline.com/your_tenant_id/v2.0

# Seoul Open Data API
SEOUL_OPEN_DATA_APP_KEY=your_seoul_open_data_api_key

# Naver API
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# AWS Credentials
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key

# CloudFront Configuration
CLOUDFRONT_KEY_PAIR_ID=your_cloudfront_key_pair_id
CLOUDFRONT_PRIVATE_KEY=your_cloudfront_private_key_here

# JWT Keys (RSA 2048-bit)
# Generate using: openssl genrsa -out private.pem 2048
# Extract public key: openssl rsa -in private.pem -pubout -out public.pem
JWT_PRIVATE_KEY=your_jwt_private_key_here
JWT_PUBLIC_KEY=your_jwt_public_key_here
